    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flexbox": "0.0.3",
    "font-awesome": "^4.7.0",
    "font-awesome-sass-loader": "^2.0.1",
    "jest": "^23.1.0",
    "jsdom": "^11.11.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.12.7",
